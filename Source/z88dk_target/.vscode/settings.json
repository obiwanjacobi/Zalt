{
    "files.associations": {
        // use Z80 Assembly vscode extension
        "*.lis": "z80-asm",
        "*.inc": "z80-asm",
        "*.m4": "z80-asm"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.obj": true,
        "**/*.exe": true,
    },
    "files.autoSave": "afterDelay",
    "C_Cpp.autoAddFileAssociations": false
}